{{ header }} {{ column_left }}  
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in  breadcrumbs %}  
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
          {% endfor %}  
      </ul>
    </div>
  </div>
  <div class="container-fluid">
     {% if  error_warning %}  
    <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
     {% endif  %}  
    {% if  success %}  
    <div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
   {% endif  %}  
    <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><i class="fa fa-pencil"></i>{{ text_list }}  </h3>
       </div>
	 
     <div class="panel-body">
      <form action="{{ delete }}" method="post" enctype="multipart/form-data" id="form">
        <table id="slide-float" class="table table-striped table-bordered table-hover">
           <thead>
            <tr><td>{{    text_image  }}</td>
                <td>{{    skinname  }}</td>
                <td>{{    status  }}</td>    
                <td class="text-right">{{ column_action }}</td>
            </tr>
          </thead>
          <tbody>
            {% if  doproocskins %}  
               {% for   skin in doproocskins  %}  
                   <tr>  
				    <td  class="left"><img    src="{{ skin.image  }}"></td>
	                <td class="left">{{   skin.skinname   }}</td>

	                <td class="left">
						  {% if  skin.status==1 %} 
							    {{ text_enabled }}
							{% else  %}  
								{{ text_disabled }} 
						   {% endif  %}  
			        </td>
					 <td class="text-right">  
					  <a href="{{ action }}{{ '&skin_id='}}{{ skin.skin_id  }}" class="btn btn-primary"> <i class="fa fa-pencil"></i></a> 
					 </td>
                 </tr>
               {% endfor  %}  
            {% else  %}  
                   <tr><td class="center" colspan="3">{{ text_no_results  }}</td>
                   </tr>
           {% endif  %}  
          </tbody>
        </table>
      </form>
    </div>
  </div>
</div>
</div> 
{{ footer }} 